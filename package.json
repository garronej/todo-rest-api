{
    "name": "todo-rest-api",
    "version": "2.0.6",
    "repository": {
        "type": "git",
        "url": "git://github.com/InseeFrLab/todo-rest-api.git"
    },
    "scripts": {
        "build": "tsx scripts/build.ts",
        "dev": "yarn build && dotenv -e .env.local -- node dist",
        "_format": "prettier \"**/*.{ts,tsx,json,md}\"",
        "format": "yarn run _format --write",
        "format:check": "yarn run _format --list-different"
    },
    "author": "u/garronej",
    "license": "MIT",
    "keywords": ["todo", "todos", "rest", "api"],
    "dependencies": {
        "@hono/node-server": "^1.11.1",
        "@hono/zod-openapi": "^0.13.0",
        "hono": "^4.3.2",
        "tsafe": "^1.7.2",
        "url-join": "^5.0.0",
        "zod": "^3.23.8",
        "oidc-spa": "^6.1.10"
    },
    "devDependencies": {
        "@types/node": "^20.12.12",
        "@vercel/ncc": "^0.38.1",
        "dotenv-cli": "^7.4.1",
        "typescript": "^5.4.5",
        "tsx": "^4.3.0"
    }
}
